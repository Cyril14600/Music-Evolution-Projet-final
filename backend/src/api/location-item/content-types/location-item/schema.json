{
    "kind": "collectionType",
    "collectionName": "location_items",
    "info": {
        "singularName": "location-item",
        "pluralName": "location-items",
        "displayName": "Location Item",
        "description": "Items for rent on the locations page"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "name": {
            "type": "string",
            "required": true
        },
        "description": {
            "type": "text"
        },
        "price": {
            "type": "string"
        },
        "category": {
            "type": "enumeration",
            "enum": [
                "decorations",
                "mobiliers"
            ],
            "required": true
        },
        "badge": {
            "type": "string"
        },
        "image": {
            "type": "media",
            "multiple": false,
            "required": false,
            "allowedTypes": [
                "images"
            ]
        }
    }
}